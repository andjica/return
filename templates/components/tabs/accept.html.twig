                                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none">
                                    <path d="M11.4343 12.7344L7.25 8.55005C6.83579 8.13583 6.16421 8.13584 5.75 8.55005C5.33579 8.96426 5.33579 9.63583 5.75 10.05L11.2929 15.5929C11.6834 15.9835 12.3166 15.9835 12.7071 15.5929L18.25 10.05C18.6642 9.63584 18.6642 8.96426 18.25 8.55005C17.8358 8.13584 17.1642 8.13584 16.75 8.55005L12.5657 12.7344C12.2533 13.0468 11.7467 13.0468 11.4343 12.7344Z" fill="currentColor"></path>
                                </svg>
                            </span>
                            <!--end::Svg Icon--></a>
                            <!--begin::Menu-->
                            <div class="menu menu-sub menu-sub-dropdown menu-column menu-rounded menu-gray-600 menu-state-bg-light-primary fw-bold fs-7 w-125px py-4" data-kt-menu="true" style="">
                                <!--begin::Menu item-->
                               
                                 <div class="menu-item px-3">
                                    <a href="{{asset('/show-process/')}}{{return.id}}" class="menu-link px-3">Show proccess</a>
                                </div>
                                <!--end::Menu item-->
                            </div>
                            <!--end::Menu-->
                        </td>
                       <td data-order="{{return.getCreatedAt()|date('Y-m-d H:i:s')}}">{{return.getCreatedAt()|date('d-m-Y')}}</td>
                        <!--end::Action=-->
                </tr>
                {% endfor %}
                </tbody>
                <!--end::Table body-->
            </table></div>
            </div>
            <!--end::Table-->
        </div>
        <!--end::Card body-->
    </div>
{% block javascripts %}

<script>    

        function searchTableAccept() {
            var input, filter, table, tr, td, i;
            input = document.getElementById("searchAccept");
            filter = input.value.toUpperCase();
            table = document.getElementById("kt_accept");
           
            tr = table.getElementsByTagName("tr");

            // Loop through all table rows, and hide those who don't match the search query
            for (i = 0; i < tr.length; i++) {
            td = tr[i].getElementsByTagName("td") ; 
            for(j=0 ; j<td.length ; j++)
            {
            let tdata = td[j] ;
            if (tdata) {
                if (tdata.innerHTML.toUpperCase().indexOf(filter) > -1) {
                tr[i].style.display = "";
                break ; 
                } else {
                tr[i].style.display = "none";
                }
            } 
            }
        }
    }
    /* $("#kt_accept").DataTable({
    "scrollY": 300,
    "scrollX": true
    }); */

    </script>
{% endblock %}